{
  "expo": {
    "name": "AIDA",
    "slug": "AIDA",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "sdkVersion": "41.0.0",
    "splash": {
      "image": "./assets/dash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.app.aida",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
